<controls:MetroWindow x:Class="PrintIt_Desktop_4.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
        Title="MainWindow" Height="350" Width="525"
        BorderThickness="0"
        GlowBrush="DarkGray"
        ShowMaxRestoreButton="False"
        TitleForeground="Black"
        WindowCloseButtonStyle="{DynamicResource CleanCloseWindowButtonStyle}"
        WindowMinButtonStyle="{DynamicResource CleanMinMaxWindowButtonStyle}"
        ShowTitleBar="True"
        Background="Black">
    <Grid>
        <WebBrowser x:Name="MapBrowser" Margin="0,0,110,0"></WebBrowser>
        <Button Width="100" Height="35" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="5" Content="Вибрати" Click="ButtonBase_OnClick"></Button>
    </Grid>
</controls:MetroWindow>
